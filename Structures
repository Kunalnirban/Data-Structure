#include<stdio.h>
#include<string.h>

struct employee
{
    int code;
    float salary;
    char name[10];

};

int main()
{
int a = 10;
char b = 'd';
float c = 234.45;

struct employee e1;
e1.code = 11;
e1.salary =34.5;
strcpy(e1.name, "kunal");

printf("%d \n",e1.code);
printf("%f \n",e1.salary);
printf("%s \n",e1.name);

return 0;
}
